{
  "name": "change-case-lite",
  "version": "0.0.2",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "vite build && npm run types",
    "build:watch": "tsc && vite build --watch",
    "types": "tsc -p ./tsconfig.declaration.json",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@vitest/coverage-v8": "2.1.3",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "vite": "^5.4.8",
    "vitest": "^2.1.3"
  },
  "keywords": [
    "change",
    "case",
    "change-case",
    "camel-case",
    "pascal-case",
    "kebab-case",
    "constant-case",
    "snake-case",
    "constant-case",
    "train-case",
    "path-case",
    "dot-case",
    "transform",
    "transform-case",
    "convert",
    "string",
    "transform-string"
  ],
  "author": "Aron Lee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aronlee0/change-case-lite.git"
  },
  "homepage": "https://github.com/aronlee0/change-case-lite#readme",
  "bugs": "https://github.com/aronlee0/change-case-lite/issues"
}
